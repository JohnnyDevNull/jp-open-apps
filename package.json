  "license": "MIT",
  "engines": {
    "node": "^20.11.0",
    "npm": "^10.2.0"
  },
  "scripts": {
    "nx": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx",
    "start-app-time-tracking": "npm run nx -- serve time-tracking",
    "start-app-household-calculator": "npm run nx -- serve household-calculator",
    "test": "npm run nx -- run-many --all --parallel=5 --target=test",
    "test:affected": "npm run nx -- affected:test --base=main --parallel=5 --runInBand",
    "e2e-app-time-tracking": "npm run nx -- e2e time-tracking",
    "e2e-app-household-calculator": "npm run nx -- e2e household-calculator",
    "build-time-tracking": "npm run nx -- build time-tracking",
    "build-household-calculator": "npm run nx -- build household-calculator",
    "lint": "npm run nx -- run-many --all --parallel=5 --target=lint",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.0"
  },
  "devDependencies": {
