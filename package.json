    "@nx/eslint-plugin": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/react": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@swc-node/register": "1.8.0",
